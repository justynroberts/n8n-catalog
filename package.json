{
  "name": "n8n-catalog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t n8n-catalog:latest .",
    "docker:run": "docker run -p 3000:3000 -v $(pwd)/data:/app/data n8n-catalog:latest",
    "docker:compose": "docker-compose up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.yml up -d",
    "docker:push": "docker push n8n-catalog:latest",
    "docker:tag": "docker tag n8n-catalog:latest",
    "export:data": "node scripts/export-data.js",
    "import:data": "node scripts/import-data.js",
    "db:fresh": "rm -f data/workflows.db* && node scripts/import-data.js"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.20.1",
    "@types/better-sqlite3": "^7.6.13",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cookies-next": "^4.2.1",
    "dexie": "^4.0.11",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.427.0",
    "mermaid": "^11.10.1",
    "next": "14.2.5",
    "openai": "^4.55.4",
    "react": "^18",
    "react-dom": "^18",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^2.5.2",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
